[info]
title=HTML Completion
desc=Handles auto-completion (Ctrl+Space) in HTML/PHP files. 1) Auto-completion with file names, when caret is inside <img src=""> tag. 2) Auto-completion with CSS class/id names, when caret is inside <tag class=""> or <tag id="">.
type=cudatext-plugin
subdir=cuda_html_completion
homepage=https://github.com/Artem3213212/cuda_html_completion
$lex=regex:.*HTML.*|PHP

[item1]
section=events
events=on_complete
lexers=$lex
