[info]
title=HTML Completion
desc=Handles auto-completion (Ctrl+Space) in HTML files. Shows auto-completion with filenames, when caret is inside value of <IMG> tag.
type=cudatext-plugin
subdir=cuda_html_completion
homepage=https://github.com/Artem3213212/cuda_html_completion
$lex=regex:.*HTML.*|PHP

[item1]
section=events
events=on_complete
lexers=$lex
